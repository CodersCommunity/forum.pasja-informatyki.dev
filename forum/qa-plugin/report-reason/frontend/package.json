{
  "name": "report-reason-plugin",
  "version": "1.0.0",
  "description": "Plugin purpose is to make descriptive reason of flagged post. Thus it will be more understandable for Administration to consider if certain post violates regulations.",
  "main": "index.js",
  "scripts": {
    "prettier": "node_modules/.bin/prettier --write index.js src/",
    "dev": "node_modules/.bin/webpack --mode=development --watch --devtool source-map index.js -o dist/script.js",
    "prebuild": "npm run prettier",
    "build": "node_modules/.bin/webpack --mode=production index.js -o dist/script.js"
  },
  "keywords": [],
  "author": "JSHolic",
  "license": "MIT",
  "devDependencies": {
    "prettier": "^2.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
